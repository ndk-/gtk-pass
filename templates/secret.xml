<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="GtkPassSecret" parent="GtkDialog">
        <child internal-child="vbox">
            <object class="GtkBox" id="gtkPassSecret">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="can_focus">False</property>
                <property name="orientation">vertical</property>
                <property name="vexpand">True</property>
                <property name="spacing">8</property>
                <property name="margin">8</property>
                <property name="margin-bottom">0</property>
                <child>
                    <object class="GtkTextView" id="gtkPassSecretView">
                        <property name="width_request">300</property>
                        <property name="height_request">300</property>
                    </object>
                </child>
                <child>
                    <object class="GtkButton" id="gtkPassSecretClose">
                        <property name="label" translatable="yes">Close</property>
                        <property name="visible">True</property>
                        <signal name="pressed" handler="on_close_click" />
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>